		"gh-pages": "^6.3.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.87.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.1.6",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.6",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"class-variance-authority": "^0.7.1",
